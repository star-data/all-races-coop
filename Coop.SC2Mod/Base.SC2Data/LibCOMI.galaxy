//--------------------------------------------------------------------------------------------------
bool libCOMI_gt_AF_SlimUI_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((libCOMI_gf_UT_DevCommandsActive(libCOMI_ge_UT_DevCommandTypes_Visual, EventPlayer()) == true))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((GameIsMissionTimePaused() == true)) {
        if ((PlayerGroupHasPlayer(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer()) == false)) {
            PlayerGroupAdd(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer());
            UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaSubtitle, StringExternal("Param/Value/lib_COMI_8BE64D65"));
        }
        else {
            PlayerGroupRemove(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer());
            UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaSubtitle, StringExternal("Param/Value/lib_COMI_085DDAFD"));
        }
    }
    else {
        if ((GameGetMissionTime() < 5.0)) {
            PlayerGroupAdd(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer());
            UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaSubtitle, StringExternal("Param/Value/lib_COMI_8520F54B"));
        }
        else {
            DialogControlHookupStandard(c_triggerControlTypePanel, "UIContainer/ConsoleUIContainer");
            if ((PlayerGroupHasPlayer(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer()) == false)) {
                PlayerGroupAdd(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer());
                UISetFrameVisible(PlayerGroupSingle(EventPlayer()), c_syncFrameTypeConsolePanel, false);
                DialogControlSendAnimationEvent(DialogControlLastCreated(), PlayerGroupSingle(EventPlayer()), "SlimUI_On");
            }
            else {
                PlayerGroupRemove(libCOMI_gv_aF_SlimUI_PlayerGroup, EventPlayer());
                UISetFrameVisible(PlayerGroupSingle(EventPlayer()), c_syncFrameTypeConsolePanel, true);
                DialogControlSendAnimationEvent(DialogControlLastCreated(), PlayerGroupSingle(EventPlayer()), "SlimUI_Off");
            }
        }
    }
    return true;
}

