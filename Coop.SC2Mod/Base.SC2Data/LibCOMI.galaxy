//--------------------------------------------------------------------------------------------------
bool libCOMI_gt_UT_Info_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((libCOMI_gf_UT_DevCommandsActive(libCOMI_ge_UT_DevCommandTypes_Visual, EventPlayer()) == true))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    TriggerExecute(libCOMI_gt_UT_InfoCreate, true, true);
    TriggerEnable(libCOMI_gt_UT_InfoButton, true);
    if ((DialogIsVisible(libCOMI_gv_uT_InfoDialog, EventPlayer()) == false)) {
        DialogSetVisible(libCOMI_gv_uT_InfoDialog, PlayerGroupSingle(EventPlayer()), true);
    }
    else {
        DialogSetVisible(libCOMI_gv_uT_InfoDialog, PlayerGroupSingle(EventPlayer()), false);
    }
    return true;
}

